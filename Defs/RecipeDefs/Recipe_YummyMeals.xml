  <RecipeDef ParentName="CookMealBase">
    <defName>ProperMeal_Bread</defName>
    <label>make bread</label>
    <description>Bake some fresh bread for the community.</description>
	<jobString>Making crunchy bread.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
	<ingredients>
		<li>
			<filter>
				<thingDefs>
					<li>IAmHungry_Flour</li>
				</thingDefs>
			</filter>
			<count>1</count>
		</li>
		<li>
			<filter>
				<thingDefs>
					<li>EggChickenUnfertilized</li>
					<li>Milk</li>
				</thingDefs>
			</filter>
			<count>1</count>
		</li>
	</ingredients>
	<products>
		<IAmHungry_Bread>5</IAmHungry_Bread>
	</products>
	<fixedIngredientFilter Inherit="False">
		<thingDefs>
			<li>IAmHungry_Flour</li>
			<li>EggChickenUnfertilized</li>
			<li>Milk</li>
		</thingDefs>
	</fixedIngredientFilter>
	<defaultIngredientFilter Inherit="False">
		<thingDefs>
			<li>IAmHungry_Flour</li>
			<li>EggChickenUnfertilized</li>
			<li>Milk</li>
		</thingDefs>
	</defaultIngredientFilter>
    <skillRequirements>
		<Cooking>8</Cooking>
    </skillRequirements>
    <researchPrerequisite>NutrientPaste</researchPrerequisite>
  </RecipeDef>

  <RecipeDef ParentName="CookMealBase">
    <defName>ProperMeal_BakedGoods</defName>
    <label>make baked goods</label>
    <description>Bake some crusty bread for the community.</description>
	<jobString>Making crusty bread.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
	<ingredients>
		<li>
			<filter>
				<thingDefs>
					<li>IAmHungry_Flour</li>
				</thingDefs>
			</filter>
			<count>1</count>
		</li>
		<li>
			<filter>
				<thingDefs>
					<li>Milk</li>
				</thingDefs>
			</filter>
			<count>0.5</count>
		</li>
		<li>
			<filter>
				<thingDefs>
					<li>EggChickenUnfertilized</li>
				</thingDefs>
			</filter>
			<count>0.5</count>
		</li>
		<li>
        <filter>
          <categories>
            <li>PlantFoodRaw</li>
          </categories>
        </filter>
        <count>1</count>
		</li>
	</ingredients>
	<products>
		<IAmHungry_BakedGoods>5</IAmHungry_BakedGoods>
	</products>
	<fixedIngredientFilter Inherit="False">
		<thingDefs>
			<li>IAmHungry_Flour</li>
			<li>EggChickenUnfertilized</li>
			<li>Milk</li>
		</thingDefs>
		<categories>
			<li>PlantFoodRaw</li>
		</categories>
	</fixedIngredientFilter>
	<defaultIngredientFilter Inherit="False">
		<thingDefs>
			<li>IAmHungry_Flour</li>
			<li>EggChickenUnfertilized</li>
			<li>Milk</li>
		</thingDefs>
		<categories>
			<li>PlantFoodRaw</li>
		</categories>
	</defaultIngredientFilter>
    <skillRequirements>
		<Cooking>12</Cooking>
    </skillRequirements>
    <researchPrerequisite>NutrientPaste</researchPrerequisite>
  </RecipeDef>

</Defs>