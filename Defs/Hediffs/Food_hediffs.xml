<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<!-- Baguette, Bread -->
  <HediffDef>
    <defName>Trunken_hediff_Bread</defName>
    <label>Saturated</label>
    <defaultLabelColor>(0.7,0.25,0.85)</defaultLabelColor>
    <hediffClass>HediffWithComps</hediffClass>
    <scenarioCanAdd>true</scenarioCanAdd>
    <maxSeverity>1</maxSeverity>
	<initialSeverity>1</initialSeverity>
    <isBad>false</isBad>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-2.0</severityPerDay>
      </li>
    </comps>
      <stages>
        <li>
          <label>Food</label>
          <hungerRateFactor>.75</hungerRateFactor>
          <restFallFactor>1.25</restFallFactor>
        </li>
      </stages>
  </HediffDef>
